<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Dynamic Form</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <form class="container" action="/dynamic" method="POST">
      <div class="text-center mt-5">
        <label>college Name</label>
        <input type="text" name="name" />
      </div>
      <div class="col-12">
        <% for (let i = 1; i <= 7; i++) { %>
        <div class="mt-4">
          <label>Branch Name</label>
          <input type="text" name="branchName_<%= i %>" />
        </div>
      </div>
      <div class="row mt-3 d-flex justify-content-around">
        <div class="col-4">
          <% for( let a = 1; a <=1; a++ ) { %>
          <div>
            <label>Eligibel</label>
            <input type="text" name="branchName_<%= i%>_eligibel" />
          </div>
        </div>
        <div class="col-4">
          <% for( let b = 1; b <=1; b++ ) { %>
          <div>
            <label>Criteria</label>
            <input type="text" name="branchName_<%= i%>_criteria" />
          </div>
        </div>
        <div class="col-4">
          <% for( let c = 1; c <=1; c++ ) { %>
          <div>
            <label>Duration</label>
            <input type="text" name="branchName_<%= i%>_duration" />
          </div>
        </div>
      </div>
      <div class="row mt-3 d-flex justify-content-around">
        <div class="col-4">
          <% for( let d = 1; d <=1; d++ ) { %>
          <div>
            <label>Tuition Fees</label>
            <input type="text" name="branchName_<%= i%>_tuitionFees" />
          </div>
        </div>
        <div class="col-4">
          <% for( let e = 1; e <=1; e++ ) { %>
          <div>
            <label>Exam</label>
            <input type="text" name="branchName_<%= i%>_exam" />
          </div>
        </div>
        <div class="col-4">
          <% for( let f = 1; f <=1; f++ ) { %>
          <div>
            <label>Hostel Fees</label>
            <input type="text" name="branchName_<%= i%>_hostelFees" />
          </div>
        </div>
      </div>
      <div class="row mt-3">
        <div class="col-12">
          <% for( let g = 1; g <=1; g++ ) { %>
          <div>
            <label>One TimeFees</label>
            <input type="text" name="branchName_<%= i%>_oneTimeFees" />
          </div>
        </div>
      </div>
      <div class="row mt-3 d-flex justify-content-around">
        <div class="col-12">
          <% for( let j = 1; j <= 7; j++ ) { %>
          <div>
            <label>Courses <%=j%></label>
            <input type="text" name="branchName_<%= i%>_coursesName_<%= j %>" />
          </div>
        </div>
      </div>
      <div class="row mt-3 d-flex justify-content-around">
        <div class="col-12">
          <% for( let k = 1; k <=1 ; k++ ) { %>
          <div>
            <label>Seat</label>
            <input
              type="text"
              name="branchName_<%= i%>_coursesName_<%= j %>_seat"
            />
          </div>
        </div>
      </div>
      <div class="row mt-3">
        <div class="col-6">
          <% for( let m = 1; m <=1 ; m++ ) { %>
          <div>
            <label>jee cutoff 2023</label>
            <input
              type="text"
              name="branchName_<%= i%>_coursesName_<%= j %>_jee_2023"
            />
          </div>
        </div>
        <div class="col-6">
          <% for( let l = 4; l ==4; l-- ) { %>
          <div>
            <label>Gujcet cutoff 202<%= l %></label>
            <input
              type="text"
              name="branchName_<%= i%>_coursesName_<%= j%>_gujcet_cutoff_202<%= l %>"
            />
          </div>
        </div>
      </div>
      <div class="row mt-3">
        <% for( let n = 1; n <=6; n++ ) { %> <% if (n==1) { %>
        <div>
          <label>Gujcet EWS cutoff 202<%= l %> </label>
          <input
            type="text"
            name="branchName_<%= i%>_coursesName_<%= j%>_gujcet_cutoff_202<%= l%>_EWS"
          />
        </div>
        <% } %> <% if (n==2) { %>
        <div>
          <label>Gujcet General cutoff 202<%= l %> </label>
          <input
            type="text"
            name="branchName_<%= i%>_coursesName_<%= j%>_gujcet_cutoff_202<%= l%>_General"
          />
        </div>
        <% } %> <% if (n==3) { %>
        <div>
          <label>Gujcet SC cutoff 202<%= l %> </label>
          <input
            type="text"
            name="branchName_<%= i%>_coursesName_<%= j%>_gujcet_cutoff_202<%= l%>_SC"
          />
        </div>
        <% } %> <% if (n==4) { %>
        <div>
          <label>Gujcet SEBC cutoff 202<%= l %> </label>
          <input
            type="text"
            name="branchName_<%= i%>_coursesName_<%= j%>_gujcet_cutoff_202<%= l%>_SEBC"
          />
        </div>
        <% } %> <% if (n==5) { %>
        <div>
          <label>Gujcet ST cutoff 202<%= l %> </label>
          <input
            type="text"
            name="branchName_<%= i%>_coursesName_<%= j%>_gujcet_cutoff_202<%= l%>_ST"
          />
        </div>
        <% } %> <% if (n==6) { %>
        <div>
          <label>Gujcet TFWS cutoff 202<%= l %> </label>
          <input
            type="text"
            name="branchName_<%= i%>_coursesName_<%= j%>_gujcet_cutoff_202<%= l%>_TFWS"
          />
        </div>
        <% } %> <% } %> <% } %> <% } %> <% } %> <% } %> <% } %><% } %><% } %><%
        } %><% } %><% } %><% } %><% } %>
      </div>
      <button type="submit">Submit</button>
    </form>
  </body>
</html>
